[timer]
command=~/.i3/i3blocks/timer
interval=5

[disk]
command=df -Th / | awk 'END{print $5}'
interval=10

[vpn]
command=~/.i3/i3blocks/vpn
interval=5

[wifi]
command=~/.i3/i3blocks/wifi
interval=5

[mail]
command=~/.i3/i3blocks/mail
interval=5

[github]
command=~/.i3/i3blocks/github
interval=5

[battery]
command=~/.i3/i3blocks/battery
interval=10

[volume]
command=~/.i3/i3blocks/volume
interval=1

[load]
command=~/.i3/i3blocks/load
interval=5

[time]
command=date '+%a %d %b %H:%M'
interval=5
